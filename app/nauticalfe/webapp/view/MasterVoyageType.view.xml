
<mvc:View
    xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    controllerName="nauticalfe.controller.MasterVoyageType"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
>
    <Page id="_IDGenPage1" showHeader="false" showFooter="false">

       
   
            <tnt:ToolHeader id="_IDGenToolHeader1"  class="sapUiTinyMargin">
                    <Button visible="true" icon="sap-icon://nav-back" type="Transparent" id="_IDGenButton2" press="onBackPress" tooltip="Backpress"/>
                    <Button visible="true" icon="sap-icon://home" type="Transparent" id="_IDGenButton3" press="onPressHome" tooltip="Home"/>
        
                    <Image id="_IDGenImage1" class="size1"
                        src="./images/ignxlogo.png"
                        height="65%"/>

                    <ToolbarSpacer id="_IDGenToolbarSpacer1"  width="15px" />
                    <Title text=" Maintain Voyage Type" wrapping="true" id="productName"/>
                    <ToolbarSpacer id="_IDGenToolbarSpacer2"/>
                    <Avatar id="_IDGenAvatar1" src="" displaySize="XS" press=".onAvatarPressed" tooltip="Profile"/>
                
            </tnt:ToolHeader>
   
       

        <Panel class="tablepanel">
            <Bar
            id="_IDGenBar1"
            class="headerBar2"  >
            <contentLeft>
                <Button id="entryBtn" text="New Entries" press="newEntries" />
                <Button id="copyBtn" text="Copy As" press="pressCopy"/>
                <Button  id= "editBtn" text="Edit"  press="pressEdit"/>
                <Button id= "deleteBtn" text="Delete" press="onDeletePress"/>
               
                <Button
                    id="button"
                    icon="sap-icon://slim-arrow-down"
                    iconFirst="false"
                    text="More"
                    press="onPress"
                    ariaHasPopup="Menu"
                />
            </contentLeft>
           
           </Bar>
      
            <ScrollContainer id="scrollContainer" height="calc(100% - 50px)" vertical="true" class="scrollContainer">
            <Table 
            id="createTypeTable"
            mode="MultiSelect"
            items="{/VoyTypeSet}"
            updateFinished="onTableUpdateFinished"
            selectionChange="selectedItems"
            width="100%"
            class="mastertables"
            sticky="ColumnHeaders"
            visible="true" 
            noDataText="Loading....">
            
            <columns>
                <Column id="_IDGenColumn1" width="30%">
                    <Text
                        id="_IDGenText11"
                        text="Voyage Code"
                    />
                  
                </Column>
                <Column id="_IDGenColumn2">
                    <Text
                        id="_IDGenText2"
                        text="Description"
                    />
                </Column>
            </columns>
            

            <items>


                <ColumnListItem id="_IDGenColumnListItem1">
                    <cells>
                     
                        <Text
                            id="_IDGenInput1"
                            text="{Voycd}"
                        />
                        <HBox >
                        <Input
                            id="_IDGenInput2"
                            value="{Voydes}"
                            editable="false"
                            maxLength="30"
                            width="300px"
 
                        />
                        <Button id="update" class="sapUiTinyMarginBegin" text="Update" icon="sap-icon://save" type="Accept" visible="false"  press="onUpdatePressBtn"></Button>
                        <Button id="delete" class="sapUiTinyMarginBegin" text="Cancel" icon="sap-icon://delete" type="Reject" visible="false" press="onCancelPressBtn"></Button>
                        </HBox>
                       
                       
                    </cells>
                </ColumnListItem>
            
            </items>
        
          </Table>
         </ScrollContainer>
        </Panel>
    

       
        
        <Table
            id="entryTypeTable"
            updateFinished="onTableUpdateFinished"
            selectionChange="selectedItems"
            width="100%"
            visible="false"
            class="tablepanel entrytables"
        
        >
            <columns>
                <Column>
                    <Text text="Voyage Code" />
                </Column>
                <Column>
                    <Text text="Description" />
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Input id="voyCode"  maxLength="4" liveChange="onCodeLiveChange"/>
                        <Input id="voyCodeDesc" liveChange="onLiveChange"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
      
        
       
        <Table
            id="updateTypeTable"
            updateFinished="onTableUpdateFinished"
            selectionChange="selectedItems"
            width="100%"
            visible="false"
            class="tablepanel "
           
        >
            <columns>
                <Column>
                    <Text text="Voyage Code" />
                </Column>
                <Column>
                    <Text text="Voyage Code Description" />
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Text id="voyCode1" text="" />
                        <Input id="voyCodeDesc1"  editable="true"  liveChange="onLiveChange"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
     
 
            <!-- main page footer -->
            <OverflowToolbar
                id="mainPageFooter"
                design="Auto"
                visible="false"
                
            >
                <ToolbarSpacer id="_IDGenToolbarSpacer3" />
                <Button
                    id="mainPageSave"
                    text="Save"
                    press="onSave"
                />
                <Button
                    id="_IDGenButton22"
                    text="Cancel"
                    press="onCancel"
                />
            </OverflowToolbar>
 
            <OverflowToolbar
                id="mainPageFooter2"
                design="Auto"
                visible="false"
            >
                <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                <Button
                    id="mainPageSave12"
                    text="Update"
                    press="onUpdate"
                />
                <Button
                    id="_IDGenButton23"
                    text="Cancel"
                    press="onCancel"
                />
            </OverflowToolbar>
 
            <!--new entries footer -->
           
       
    </Page>
 
   
   
</mvc:View>