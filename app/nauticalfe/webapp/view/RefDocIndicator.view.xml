<mvc:View controllerName="nauticalfe.controller.RefDocIndicator"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
   <Page id="page">
        <headerContent>
            <Bar id="_IDGenBar2">
                <contentLeft>
                    <core:Icon id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        press="onBackPress"
                    />
                    <core:Icon id="_IDGenIcon2"
                        src="sap-icon://home"
                        press="onPressHome"
                    />
                   
                    <Text id="_IDGenText1"
                        class="text1"
                        text="Ref Doc Indicator"
                    />
                </contentLeft>
                
            </Bar>
        </headerContent>
        <Panel id="_IDGenPanel1" class="tablepanel">
        <Bar id="_IDGenBar1" class="headerBar2" >
            <contentLeft>
                <Button id="_IDGenButto1"
                    text="New Entries"
                    press="onCreatePress"
                />
