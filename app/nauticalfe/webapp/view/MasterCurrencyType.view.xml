<mvc:View
    xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    xmlns:l="sap.ui.layout"
    controllerName="nauticalfe.controller.MasterCurrencyType"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
   
>
    <Page id="_IDGenPage1">
        <headerContent>
            <!-- Your header content goes here -->
            <Bar
                id="_IDGenBar1"
                class="customHeader" >
                <contentLeft>
                    <core:Icon
                        id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPress"
                    />
                    <core:Icon
                        id="_IDGenIcon2"
                        src="sap-icon://home"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPressHome"
                       
                    />
                    <Image
                         class="size1"
                        id="_IDGenImage2"
                        src="./images/ignxlogo.png"
                        height="75%"
                    />
                    <Text
                        id="_IDGenText1"
                        class="textBid size1"
                        text='Change View Currency Master Data: Overview'
                    />
                </contentLeft>
                <contentRight>
                    <core:Icon
                        id="_IDGenIcon3"
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData
                                id="_IDGenFlexItemData1"
                                growFactor="1"
                            />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon
                        id="_IDGenIcon4"
                        src="sap-icon://SAP-icons-TNT/user"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData
                                id="_IDGenFlexItemData2"
                                growFactor="1"
                            />
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>
        <Bar
            id="_IDGenBar12"
            class="_IDGenBar1" >
            <contentLeft>
                <Button id="entryBtn"
                    text="New Entries"
                    press="newEntries"
                />
                <Button  id="copyBtn" text="Copy As" press="pressCopy" />
                <Button id="editBtn"  text="Edit"  press="pressEdit"/>
                <Button  id="deleteBtn" text="Delete" press="onDeletePress"/>
               
                <Button
                    id="button"
                    icon="sap-icon://slim-arrow-down"
                    iconFirst="false"
                    text="More"
                    press="onPress"
                    ariaHasPopup="Menu"
                />
            </contentLeft>
        </Bar>
 
        <Label
            text="Currency Type Master Table "
            class="masterheadings"
        />
        <Table
            id="createTypeTable"
            mode="MultiSelect"
            items="{/CurTypeSet}"
            updateFinished="onTableUpdateFinished"
            selectionChange="selectedItems"
            width="100%"
            class="mastertables"
            noDataText="Loading ..."
            enableBusyIndicator="true"
           
         >
            <columns>
                <Column id="_IDGenColumn1" width="400px">
                    <Text
                        id="_IDGenText11"
                        text="Currency"
                    />
                </Column>
                <Column id="_IDGenColumn2">
                    <Text
                        id="_IDGenText2"
                        text="Currency Description"
                    />
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem1">
                    <cells>
                     
                        <Text
                            id="_IDGenInput1"
                            text="{Navoycur}"
                        />
                        <HBox >
                        <Input
                            id="DescInput"
                            value="{Navoygcurdes}"
                            editable="false"
                             maxLength="30"
                           
                        />
                       
                        <Button id="update" class="sapUiTinyMarginBegin" text="Update" icon="sap-icon://save" type="Accept" visible="false"  press="onUpdate"></Button>
                        <Button id="delete" class="sapUiTinyMarginBegin" text="Cancel" icon="sap-icon://delete" type="Reject" visible="false" press="onCancel"></Button>
 
 
                        </HBox>
                       
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <Table
            id="entryTypeTable"
            updateFinished="onTableUpdateFinished"
            selectionChange="selectedItems"
            width="100%"
            class="mastertables"
            visible="false"
        >
            <columns>
                <Column>
                    <Text text="Currency" />
                </Column>
                <Column>
                    <Text text="Currency Description" />
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Input id="NAVOYCUR2" maxLength="3"/>
                        <Input id="NAVOYGCURDES2" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <Table
            id="updateTypeTable"
            updateFinished="onTableUpdateFinished"
            selectionChange="selectedItems"
            width="100%"
            class="mastertables"
            visible="false"
           
        >
            <columns>
                <Column>
                    <Text text="Currency Type" />
                </Column>
                <Column>
                    <Text text=" Currency Description" />
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Text id="NAVOYCUR1" text="" />
                        <Input id="NAVOYGCURDES1"  editable="true" liveChange="OndescriptionChanged"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <OverflowToolbar
                id="mainPageFooter"
                design="Auto"
                visible="false"
            >
                <ToolbarSpacer id="_IDGenToolbarSpacer3" />
                <Button
                    id="mainPageSave"
                    text="Save"
                    press="onSave"
                />
                <Button
                    id="_IDGenButton22"
                    text="Cancel"
                    press="onCancel"
                />
            </OverflowToolbar>
        <OverflowToolbar
                id="mainPageFooter2"
                design="Auto"
                visible="false"
            >
                <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                <Button
                    id="mainPageSave12"
                    text="Update"
                    press="onUpdate"
                />
                <Button
                    id="_IDGenButton23"
                    text="Cancel"
                    press="onCancel"
                />
            </OverflowToolbar>
    </Page>
</mvc:View>