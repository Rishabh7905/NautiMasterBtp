<mvc:View
    xmlns:table="sap.ui.table"
    xmlns:cards="sap.f.cards"
    controllerName="nauticalfe.controller.PortLocUpload"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
>
    <Page id="page">
        <headerContent>
            <!-- Your header content goes here -->
            <Bar class="customHeader">
                <contentLeft>
                    <core:Icon
                        src="sap-icon://nav-back"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPress"
                    />
                    <core:Icon
                        src="sap-icon://home"
                        class="size1"
                        color="#FFFFFF"
                        press="onBackPressHome"
                    />
                    <Image
                        class="size1"
                        src="./images/ignxlogo.png"
                        height="75%"
                    />
                    <Text
                        class="textBid size1"
                        text="Upload Program of Port"
                    />
                </contentLeft>
                <contentRight>
                    <core:Icon
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon
                        src="sap-icon://SAP-icons-TNT/user"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                </contentRight>
            </Bar>
        </headerContent>
        <Bar
            id="_IDGenBar1"
            class="_IDGenBar1"
        >
            <contentLeft>
                <!-- <Button
                    id="_IDGenButton1"
                    text="Save as Variant"
                    press="onSaveAs"
                /> -->
                <Button
                    id="_IDGenButton2"
                    text="Download as Template"
                    press="onDownloadPress"
                />
                <!-- <Button
                    id="button"
                    icon="sap-icon://slim-arrow-down"
                    iconFirst="false"
                    text="More"
                    press="onPress"
                    ariaHasPopup="Menu"
                /> -->
            </contentLeft>
           
        </Bar>
 
        <HBox alignContent="Stretch">
            <unified:FileUploader
                id="fileUploader"
                uploadUrl="/"
                buttonText="Choose File"
                fileType="xlsx"
                uploadComplete="onUploadComplete"
                change="onFileChange"
            />
            <Button
                text="Upload"
                press="onUploadPress"
            />
            <Button
                text="Preview"
                press="onPreviewPress"
            />
        </HBox>
 
        <footer>
            <OverflowToolbar design="Solid">
                <ToolbarSpacer />
                <Button
                    text="Execute"
                    type="Negative"
                    icon="none"
                />
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>
 