<mvc:View
    xmlns:core="sap.ui.core"
    controllerName="nauticalfe.controller.PortLocMaster"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page id="page">
        <headerContent>
            <Bar id="_IDGenBar2">
                <contentLeft>
                    <core:Icon id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        press="onBackPress"
                    />
                    <core:Icon id="_IDGenIcon2"
                        src="sap-icon://home"
                        press="onPressHome"
                    />
                   
                    <Text id="_IDGenText1"
                        class="text1"
                        text="Port Location Master"
                    />
                </contentLeft>
                
            </Bar>
        </headerContent>

        <Panel id="_IDGenPanel1" class="tablepanel">
        <Bar id="_IDGenBar12" class="headerBar2">
            <contentLeft>
                <Button
                    id="_IDGenButton12"
                    text="New Entries"
                    press="newEntries"
                />
                <Button
                    id="_IDGenButton13"
                    text="Copy As"
                    press="pressCopy"
                />
                <Button
                    id="_IDGenButton14"
                    text="Delete"
                    press="onDeletePress"
                />
                <Button
                    id="_IDGenButton15"
                    text="Edit"
                    press="pressEdit"
                />
               
            </contentLeft>
        </Bar>

        <ScrollContainer id="scrollContainer" height="calc(100% - 50px)" vertical="true" class="scrollContainer">

        <Table id="createTypeTable"
            width="100%"
            class="mastertables"
            items="{/PORTLOC}"
            mode="MultiSelect"
            selectionChange="selectedItems">

            <columns>
                <Column >
                    <Label text="Country" />
                </Column>
                <Column >
                    <Label text="Port Code" />
                </Column>
                <Column >
                    <Label text="Port Name" />
                </Column>
                <Column >
                    <Label text="Related Anchorage" />
                </Column>
                 <Column  >
                    <Label text="Latitude" />
                </Column>
                <Column >
                    <Label text="Longitude" />
                </Column>
                <Column >
                    <Label text="Country Name" />
                </Column>
                <Column >
                    <Label text="Location ID" />
                </Column>
                <Column >
                    <Label text="Proc ind" />
                </Column>
               
            </columns>
            <ColumnListItem id="_istItem1">
                <cells>
                   
                    <Text id="_IDGenText3" text="{COUNTRY}" />
                    <Text id="_IDGenText4" text="{PORTC}" />
                    <Text id="_IDGenText5" text="{PORTN}" />
                    <Text id="_IDGenText6" text="{REANCHO}" />
                    <Text id="_IDGenText7" text="{LATITUDE}" />
                    <Text id="_IDGenText8" text="{LONGITUDE}" />
                    <Text id="_IDGenText9" text="{COUNTRYN}" />
                    <Text id="_IDGenText10" text="{LOCID}" />
                    <Text id="_IDGenText11" text="{IND}" />
                </cells>
            </ColumnListItem>
        </Table>
        </ScrollContainer>
        </Panel>

        <Table id="entryTypeTable"
            selectionChange="selectedItems"
            width="100%"
            class="tablepanel entrytables"
            visible="false"
        >
            <columns>
                <Column >
                    <Label text="Country" />
                </Column>
                <Column >
                    <Label text="Port Code" />
                </Column>
                <Column >
                    <Label text="Port Name" />
                </Column>
                <Column >
                    <Label text="Related Anchorage" />
                </Column>
                 <Column  >
                    <Label text="Latitude" />
                </Column>
                <Column >
                    <Label text="Longitude" />
                </Column>
                <Column >
                    <Label text="Country Name" />
                </Column>
                <Column >
                    <Label text="Location ID" />
                </Column>
                <Column >
                    <Label text="Proc ind" />
                </Column>
               
            </columns>
            <ColumnListItem id="istItem1">
                <cells>
                    <Input id="COUNTRY"/>
                    <Input id="PORTC" />
                    <Input id="PORTN"/>
                    <Input id="REANCHO"/>
                    <Input id="LATITUDE"/>
                    <Input id="LONGITUDE" />
                    <Input id="COUNTRYN"/>
                    <Input id="LOCID" />
                    <Input id="IND"/>
                </cells>
            </ColumnListItem>
        </Table>

        <Table
            id="updateTypeTable"
            selectionChange="selectedItems"
            width="100%"
            class="mastertables"
            visible="false" >
            <columns>
                <Column >
                    <Label text="Country" />
                </Column>
                <Column >
                    <Label text="Port Code" />
                </Column>
                <Column >
                    <Label text="Port Name" />
                </Column>
                <Column >
                    <Label text="Related Anchorage" />
                </Column>
                 <Column  >
                    <Label text="Latitude" />
                </Column>
                <Column >
                    <Label text="Longitude" />
                </Column>
                <Column >
                    <Label text="Country Name" />
                </Column>
                <Column >
                    <Label text="Location ID" />
                </Column>
                <Column >
                    <Label text="Proc ind" />
                </Column>
            </columns>
            <items>
                <ColumnListItem id="stItem1">
                    <cells>
                        <Input id="country" value="" editable="false"/>
                        <Input id="portc" />
                        <Input id="portn" />
                        <Input id="reancho" />
                        <Input id="latitude" />
                        <Input id="longitude" />
                        <Input id="countryn" />
                        <Input id="locid" />
                        <Input id="ind" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
            <OverflowToolbar
                id="mainPageFooter2"
                design="Auto"
                visible="false" >
                <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                <Button
                    id="mainPageSave12"
                    text="Update"
                    press="onUpdate"
                />
                <Button
                    id="_IDGenButton23"
                    text="Cancel"
                    press="onCancel"
                />
            </OverflowToolbar>
 
 
            <!-- main page footer -->
            <OverflowToolbar
                id="mainPageFooter"
                design="Auto"
                visible="false">
                <ToolbarSpacer id="_IDGenToolbarSpacer3" />
                <Button id="add" icon="sap-icon://sys-add" press=".onAddRow1" text="Add Row" type="Success"></Button>
                <Button id="minus" icon="sap-icon://sys-minus" press=".onDeleteRow1" text="Delete Row" type="Reject"></Button>

                <Button
                    id="mainPageSave"
                    text="Save"
                    press="onSave"
                />
                <Button
                    id="_IDGenButton22"
                    text="Cancel"
                    press="onCancel"
                />
            </OverflowToolbar>
       
        
       
 
       
    </Page>
</mvc:View>
 